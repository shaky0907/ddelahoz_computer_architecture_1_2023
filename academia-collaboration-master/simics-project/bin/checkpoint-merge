if [ -f "/home/david/Downloads/academia-collaboration-master/simics-project/.package-list" ]; then
    exec "/home/david/Documents/SIMICSS/simicsinstall/simics-6.0.166/bin/checkpoint-merge" --package-list "/home/david/Downloads/academia-collaboration-master/simics-project/.package-list" --project "/home/david/Downloads/academia-collaboration-master/simics-project" "$@"
else
    exec "/home/david/Documents/SIMICSS/simicsinstall/simics-6.0.166/bin/checkpoint-merge" --project "/home/david/Downloads/academia-collaboration-master/simics-project" "$@"
fi
